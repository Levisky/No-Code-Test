ner {\n  z-index: 10;\n  position: absolute;\n}\n.hamburger-menu_1644237468695_616.open .nav-container {\n}\n\n/* hamburger button */\n.hamburger-menu_1644237468695_616 .hamburger-btn {\n    z-index: 20;\n    cursor: pointer;\n    position: absolute;\n}\n.hamburger-menu_1644237468695_616 .hamburger-btn span {\n    display: block;\n    width: 30px;\n    height: 5px;\n    margin-bottom: 3px;\n    position: relative;\n    background-color: #000000;\n    border: 1px solid #ffffff;\n    border-radius: 3px;\n    transition: border-color 0.1s ease-in,\n      opacity 0.1s ease-in,\n      transform 0.1s ease-in,\n      background 0.1s ease-in\n}\n\n.hamburger-menu_1644237468695_616.open .hamburger-btn span {\n    border-color: rgba(255, 255, 255, 0);\n}\n\n.hamburger-menu_1644237468695_616 .hamburger-btn span:first-child {\n    transform-origin: 4px -2px;\n}\n.hamburger-menu_1644237468695_616.open .hamburger-btn span:first-child {\n    transform: rotate(45deg) translate(7px, -4px);\n}\n\n.hamburger-menu_1644237468695_616.open .hamburger-btn span:nth-last-child(2) {\n    opacity: 0;\n}\n\n.hamburger-menu_1644237468695_616 .hamburger-btn span:nth-child(3) {\n    transform-origin: -11px 4px;\n}\n.hamburger-menu_1644237468695_616.open .hamburger-btn span:nth-child(3) {\n    transform: rotate(-45deg) translate(0, 12px);\n}\n\n/* themes */\n\n\n</style>\n<!-- inspired by https://codepen.io/erikterwan/pen/EVzeRP -->\n<div class=\"hamburger-menu_1644237468695_616 closed\">\n    <div class=\"hamburger-btn \">\n      <span></span>\n      <span></span>\n      <span></span>\n    </div>\n    <div class=\"nav-container\">\n      <div class=\"prevent-scale\">\n        <nav role=\"navigation\" class=\"\">\n            <ul>\n            \n            </ul>\n        </nav>\n      </div>\n  </div>\n</div>\n<script>\n(function() {\n  var isOpen = false;\n  function close() {\n    isOpen = false;\n    $('.hamburger-menu_1644237468695_616').removeClass('open');\n    $('.hamburger-menu_1644237468695_616').addClass('closed');\n  }\n  function open() {\n    isOpen = true;\n    $('.hamburger-menu_1644237468695_616').addClass('open');\n    $('.hamburger-menu_1644237468695_616').removeClass('closed');\n  }\n  function toggle() {\n    if(isOpen) close();\n    else open();\n  }\n  var silexElement = $('.hamburger-menu_1644237468695_616').closest('.editable-style');\n  function move(data) {\n    var offset = silexElement.offset();\n    $('.hamburger-menu_1644237468695_616 .nav-container').css({\n      'transform': 'translate(' + (data.scrollLeft - (offset.left/data.scale)) + 'px, ' + (data.scrollTop - (offset.top/data.scale)) + 'px)'\n    });\n  }\n  $('.hamburger-menu_1644237468695_616 .hamburger-btn').click(function() { toggle(); });\n  $(document).on('silex.scroll silex.resize', function(e) {\n    close();\n    move(window.silex);\n  });\n  close();\n})();\n</script>\n"}]}